super + Return
	terminal

super + control + Return
	terminal-with-cwd

super + r
	modal-run -matching normal

super + g
	DB=~/.guts/.config/bookmarks.yaml bookmarks

control + super + g
	DB=~/.guts/.config/bookmarks.yaml bookmarks add

super + shift + r
	modal-run --terminal

shift + Delete
	kill-focus

super + control + s
	browser

super + control + w
	mrun spotify

super + F1
	screenshot

super + F2
	screenshot deepin

control + F10
	brightness toggle

control + F12
	bluetooth-toggle

super + 8
	;volume down

super + 9
	;volume up

super + Left
	;volume down

super + Down
	;volume down

super + Up
	;volume up

super + Right
	;volume up

super + control + 9
	;pa-switch-sink && notify "$(pactl-get-default-sink)"

super + control + 0
	;pa-switch-profile -i webcam active output:hdmi-stereo output:analog-stereo

alt + control + d
	;infobar

super + control + e
	translator

alt + Tab
	i3re -w 20 || i3re -h 20

super + Tab
	xoal

super + 5
	carcosa-ui ~/.secrets:me ~/reconquest/secrets:re

control + super + 5
	carcosa-ui ~/.secrets:me ~/reconquest/secrets:re --mode login-password

super + 6
	carcosa-new-password

super + 7
	secrets_repository=~/.secrets carcosa-qr

super + grave
	;kuake-bottom

super + backslash
	quickaction

super + BackSpace
	chrome-todoist

super + equal
	;kuake --terminal --class top --top --middle --height 30 --width 50

