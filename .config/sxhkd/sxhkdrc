	tdrop -a -m -w 50% -x 25% -y 60% -h 40% alacritty --class kuake -e tmux

super + backslash
	quickaction

super + equal
	tdrop -a -m -w 50% -x 25% -h 40% alacritty --class kuake -e tmux

